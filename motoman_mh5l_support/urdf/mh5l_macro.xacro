<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
	<xacro:property name="pi" value="3.141592653589793" />

	<xacro:macro name="motoman_mh5l" params="prefix">

		<!-- link list -->
		<link name="${prefix}base_link">
			<visual>
				<geometry>
					<mesh filename="package://motoman_mh5l_support/meshes/mh5l/visual/MH5L_BASE_AXIS.stl" />
				</geometry>
				<material name="blue">
					<color rgba="0 0 1 1"/>
				</material>
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://motoman_mh5l_support/meshes/mh5l/collision/MH5L_BASE_AXIS.stl" />
				</geometry>
				<material name="yellow">
					<color rgba="0 1 1 1"/>
				</material>
			</collision>
		</link>

		<link name="${prefix}link_s">
			<visual>
				<geometry>
					<mesh filename="package://motoman_mh5l_support/meshes/mh5l/visual/MH5L_S_AXIS.stl"/>
				</geometry>
				<material name="silver">
					<color rgba="0.8 0.8 0.8 1"/>
				</material>
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://motoman_mh5l_support/meshes/mh5l/collision/MH5L_S_AXIS.stl" />
				</geometry>
				<material name="yellow"/>
			</collision>
		</link>

		<link name="${prefix}link_l">
			<visual>
				<geometry>
					<mesh filename="package://motoman_mh5l_support/meshes/mh5l/visual/MH5L_L_AXIS.stl"/>
				</geometry>
				<material name="blue"/>
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://motoman_mh5l_support/meshes/mh5l/collision/MH5L_L_AXIS.stl" />
				</geometry>
				<material name="yellow"/>
			</collision>
		</link>

		<link name="${prefix}link_u">
			<visual>
				<geometry>
					<mesh filename="package://motoman_mh5l_support/meshes/mh5l/visual/MH5L_U_AXIS.stl"/>
				</geometry>
				<material name="silver"/>
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://motoman_mh5l_support/meshes/mh5l/collision/MH5L_U_AXIS.stl" />
				</geometry>
				<material name="yellow"/>
			</collision>
		</link>

		<link name="${prefix}link_r">
			<visual>
				<geometry>
					<mesh filename="package://motoman_mh5l_support/meshes/mh5l/visual/MH5L_R_AXIS.stl"/>
				</geometry>
				<material name="blue"/>
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://motoman_mh5l_support/meshes/mh5l/collision/MH5L_R_AXIS.stl" />
				</geometry>
				<material name="yellow"/>
			</collision>
		</link>

		<link name="${prefix}link_b">
			<visual>
				<geometry>
					<mesh filename="package://motoman_mh5l_support/meshes/mh5l/visual/MH5L_B_AXIS.stl"/>
				</geometry>
				<material name="silver"/>
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://motoman_mh5l_support/meshes/mh5l/collision/MH5L_B_AXIS.stl" />
				</geometry>
				<material name="yellow"/>
			</collision>
		</link>

		<link name="${prefix}link_t">
			<visual>
				<geometry>
					<mesh filename="package://motoman_mh5l_support/meshes/mh5l/visual/MH5L_T_AXIS.stl" />
				</geometry>
				<material name="blue"/>
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://motoman_mh5l_support/meshes/mh5l/collision/MH5L_T_AXIS.stl" />
				</geometry>
				<material name="yellow"/>
			</collision>
		</link>
		<!-- end of link list -->

		<!-- joint list -->
		<joint name="${prefix}joint_s" type="revolute">
			<parent link="${prefix}base_link"/>
			<child link="${prefix}link_s"/>
			<origin xyz="0 0 0" />
			<axis xyz="0 0 1" />
			<limit lower="${-170 * pi / 180}" upper="${170 * pi / 180}" effort="100" velocity="${270 * pi / 180}" />
		</joint>

		<joint name="${prefix}joint_l" type="revolute">
			<parent link="${prefix}link_s"/>
			<child link="${prefix}link_l"/>
			<origin xyz="0 0.088 0.330" />
			<axis xyz="-1 0 0" />
			<limit lower="${-65 * pi / 180}" upper="${150 * pi / 180}" effort="100" velocity="${280 * pi / 180}" />
		</joint>

		<joint name="${prefix}joint_u" type="revolute">
			<parent link="${prefix}link_l"/>
			<child link="${prefix}link_u"/>
			<origin xyz="0 0 0.400" />
			<axis xyz="1 0 0" />
			<!-- These limits are smaller than the datasheet, but the datasheet limits are in collision. -->
			<!-- Datasheet says [-138, 255] degrees. -->
			<limit lower="${-138 * pi / 180}" upper="${255 * pi / 180}" effort="100" velocity="${300 * pi / 180}" />
		</joint>

		<joint name="${prefix}joint_r" type="revolute">
			<parent link="${prefix}link_u"/>
			<child link="${prefix}link_r"/>
			<origin xyz="0 0 0.040" />
			<axis xyz="0 1 0" />
			<limit lower="${-190 * pi / 180}" upper="${190 * pi / 180}" effort="100" velocity="${450 * pi / 180}" />
		</joint>

		<joint name="${prefix}joint_b" type="revolute">
			<parent link="${prefix}link_r"/>
			<child link="${prefix}link_b"/>
			<origin xyz="0 0.405 0" />
			<axis xyz="-1 0 0" />
			<limit lower="${-135 * pi / 180}" upper="${135 * pi / 180}" effort="100" velocity="${450 * pi / 180}" />
		</joint>

		<joint name="${prefix}joint_t" type="revolute">
			<parent link="${prefix}link_b"/>
			<child link="${prefix}link_t"/>
			<origin xyz="0 0.080 0" />
			<axis xyz="0 1 0" />
			<limit lower="${-360 * pi / 180}" upper="${360 * pi / 180}" effort="100" velocity="${720 * pi / 180}" />
		</joint>
		<!-- end of joint list -->

	</xacro:macro>
</robot>

